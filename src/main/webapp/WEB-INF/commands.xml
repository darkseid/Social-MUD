<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:context="http://www.springframework.org/schema/context"
	xmlns:mvc="http://www.springframework.org/schema/mvc"
	xsi:schemaLocation="http://www.springframework.org/schema/mvc http://www.springframework.org/schema/mvc/spring-mvc-3.0.xsd
		http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-3.0.xsd
		http://www.springframework.org/schema/context http://www.springframework.org/schema/context/spring-context-3.0.xsd">
	
	<bean id="lookCommand" class="com.smud.web.model.command.LookCommand"/>
		
	<bean id="commandsService" class="com.smud.service.DefaultCommandsService">
		<property name="commands">
			<map>
				<entry key="look" value-ref="lookCommand"/>
			</map>
		</property>
	</bean>
	
	<bean id="mockRoom"	class="com.smud.model.Room">
		<property name="title" value="mock room"/>
		<property name="description" value="mock"/>
	</bean>
		
</beans>